{
  "name": "netlify-identity-redirect-check",
  "version": "1.0.0",
  "description": "To check if the 404 redirects work on login and if logged in, the right page shows up.",
  "author": "Sachin Sancheti",
  "scripts": {
    "dev": "npm-run-all clean dev:11ty",
    "build": "cross-env npm-run-all -p build:*",
    "build:11ty": "eleventy",
    "dev:11ty": "eleventy --serve --quiet",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sachinsancheti1/netlify-identity-redirect-check"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-cache-assets": "^2.1.0",
    "@11ty/eleventy-img": "^0.7.8",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "css-purge": "^3.1.8",
    "csso": "^4.2.0",
    "datauri": "^3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "html-minifier": "^4.0.0",
    "luxon": "^1.26.0",
    "purgecss": "^2.3.0"
  },
  "devDependencies": {
    "clean-css": "^4.2.3",
    "core-js": "^2.6.12",
    "cross-env": "^6.0.3",
    "es6-promisify": "^6.1.1",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "posthtml": "^0.13.4",
    "posthtml-minify-classnames": "^0.2.3",
    "rimraf": "^3.0.2"
  }
